#!groovy
properties([disableConcurrentBuilds()])

pipeline {
    agent any
    environment {
        PROJECT_NAME = "${GIT_URL.tokenize('/.')[-2]}"
        BRANCH = "${GIT_BRANCH.split('/')[1]}"
    }
    stages {
        stage('Build '){
            steps {
                script {
                         echo "Build succes"
                         echo "${BRANCH}"
                         echo "${PROJECT_NAME}"
                    }
                }         
        }  
    }
}
