#!groovy
properties([disableConcurrentBuilds()])

pipeline {
    agent any
    stages {
        stage('Build '){
            steps {
                script {
           
                        echo "Build succes"
                         echo "${GIT_COMMIT}"
                         echo "GIT_COMMIT ${GIT_COMMIT}" 
                         echo "GIT_BRANCH ${GIT_BRANCH}"
                            echo "GIT_LOCAL_BRANCH ${GIT_LOCAL_BRANCH}"
                            echo "GIT_PREVIOUS_COMMIT ${GIT_PREVIOUS_COMMIT}"
                    echo "GIT_URL ${GIT_URL}"
                      
                    }
                }         
        }  
    }
}
