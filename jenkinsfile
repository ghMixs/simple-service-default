#!groovy
properties([disableConcurrentBuilds()])

pipeline {
    agent any
    stages {
        stage('Build '){
            steps {
                script {
                         echo "Build succes"
                         echo "GIT_COMMIT ${GIT_COMMIT}" 
                         basename=$(basename $GIT_COMMIT)
                         echo $basename
                         echo "GIT_BRANCH ${GIT_BRANCH.split('/')[1]}"
                         echo "GIT_URL ${GIT_URL}"
                      
                    }
                }         
        }  
    }
}
