#!groovy
properties([disableConcurrentBuilds()])
environment {
        PROJECT_NAME = ${GIT_URL.tokenize('/.')[-2]}
        BRANCH = ${GIT_BRANCH.split('/')[1]
}
pipeline {
    agent any
    stages {
        stage('Build '){
            steps {
                script {
                         echo "Build succes"
                         echo "${BRANCH}"
                         echo "${PROJECT_NAME}"
                    }
                }         
        }  
    }
}
